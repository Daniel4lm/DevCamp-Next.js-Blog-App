{
  "name": "instacamp",
  "version": "0.5.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:test": "pnpm dotenv -e .env.test start-server-and-test dev 3000 cypress:open",
    "cypress:run": "cypress run",
    "migrate:dev": "pnpm dotenv -e .env.local -- prisma migrate dev",
    "db:push": "pnpm dotenv -e .env.local -- prisma db push",
    "migrate:reset": "pnpm dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "pnpm dotenv -e .env.local -- prisma db seed",
    "db:seed_test": "pnpm dotenv -e .env.test -- prisma db seed",
    "prisma:generate": "pnpm dotenv -e .env.local -- prisma generate",
    "prisma:test_generate": "pnpm dotenv -e .env.test -- prisma generate",
    "prisma:test_migrate": "pnpm dotenv -e .env.test -- prisma migrate dev",
    "prisma:studio": "pnpm dotenv -e .env.local -- prisma studio",
    "production:build": "pnpm prisma generate && pnpm prisma migrate deploy && next build"
  },
  "prisma": {
    "seed": "pnpm dotenv -e .env.local -- ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "seed_test": "pnpm dotenv -e .env.test -- ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@next/env": "^13.5.4",
    "@prisma/client": "^5.2.0",
    "@tanstack/query-core": "4.29.19",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-query-devtools": "^4.29.19",
    "@tinymce/tinymce-react": "^4.3.0",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.4.16",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.8",
    "eslint-config-next": "14.0.1",
    "form-data": "^4.0.0",
    "formik": "^2.4.2",
    "highlight.js": "^11.8.0",
    "mailgun.js": "^9.2.1",
    "next": "^13.5.1",
    "next-auth": "^4.23.1",
    "next-sitemap": "^4.2.2",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-quill": "^2.0.0",
    "sharp": "^0.32.5",
    "tailwindcss": "3.3.5",
    "tinymce": "^6.7.0",
    "zod": "^3.21.4",
    "zod-formik-adapter": "^1.2.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.32.5",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.2.5",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "cypress": "^13.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.48.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-testing-library": "^6.0.1",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "msw": "^2.0.3",
    "prisma": "^5.2.0",
    "start-server-and-test": "^2.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "undici": "^5.27.2",
    "whatwg-fetch": "^3.6.18"
  }
}
