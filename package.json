{
  "name": "instacamp",
  "version": "0.4.17",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "migrate:dev": "pnpm dotenv -e .env.local -- prisma migrate dev",
    "db:push": "pnpm dotenv -e .env.local -- prisma db push",
    "migrate:reset": "pnpm dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "pnpm dotenv -e .env.local -- prisma db seed",
    "prisma:generate": "pnpm dotenv -e .env.local -- prisma generate",
    "prisma:studio": "pnpm dotenv -e .env.local -- prisma studio",
    "production:build": "pnpm prisma generate && pnpm prisma migrate deploy && next build"
  },
  "prisma": {
    "seed": "pnpm dotenv -e .env.local -- ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@prisma/client": "^4.16.2",
    "@tanstack/query-core": "4.29.19",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-query-devtools": "^4.29.19",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.8",
    "eslint-config-next": "13.4.4",
    "form-data": "^4.0.0",
    "formik": "^2.4.2",
    "highlight.js": "^11.8.0",
    "mailgun.js": "^9.2.1",
    "next": "^13.4.16",
    "next-auth": "^4.22.1",
    "next-sitemap": "^4.1.3",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-quill": "^2.0.0",
    "sharp": "^0.32.1",
    "tailwindcss": "3.3.2",
    "zod": "^3.21.4",
    "zod-formik-adapter": "^1.2.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.32.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "prisma": "^4.16.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
